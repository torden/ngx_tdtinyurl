--TEST--
Check for tinyurl presence
--SKIPIF--
<?php if (!extension_loaded("tinyurl")) print "skip"; ?>
--FILE--
<?php 
print '<h1>Bucket API Test</h1><br>';
print '<h3>Public API</h3><br>';
print 'Call tinyurl_is_enable_user : <b>'. tinyurl_is_enable_user() .'<b><br>';
print 'Call tinyurl_get_current_group : <b>'. tinyurl_get_current_group() .'<b><br>';

print '<h3>Private API for Internal User</h3><br>';
print 'Call _tinyurl_get_tinyurl_id : <b>'. _tinyurl_get_tinyurl_id() .'<b><br>';
print 'Call _tinyurl_get_prefix_group_no : <b>'. _tinyurl_get_prefix_group_no() .'<b><br>';
?>
--EXPECT--
tinyurl extension is available
